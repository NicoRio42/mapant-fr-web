{
	"name": "mapant-fr-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev:stripe": "stripe listen --forward-to localhost:5173/api/stripe/webhook --skip-verify",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db:studio": "drizzle-kit studio",
		"db:migrations:generate": "drizzle-kit generate",
		"db:migrations:apply:local": "drizzle-kit migrate",
		"db:migrations:apply:staging": "bun --env-file=.env.staging drizzle-kit migrate --config=drizzle-prod.config.ts",
		"db:migrations:apply:prod": "bun --env-file=.env.prod drizzle-kit migrate --config=drizzle-prod.config.ts",
		"db:seed:local": "bun ./src/scripts/insert-tiles.ts",
		"db:seed:staging": "bun --env-file=.env.staging ./src/scripts/insert-tiles.ts",
		"db:seed:prod": "bun --env-file=.env.prod ./src/scripts/insert-tiles.ts",
		"db:runSql:local": "bun ./src/scripts/run-sql.ts",
		"db:runSql:staging": "bun --env-file=.env.staging ./src/scripts/run-sql.ts",
		"db:runSql:prod": "bun --env-file=.env.prod ./src/scripts/run-sql.ts",
		"test": "vitest",
		"wrangler": "wrangler",
		"miniflare": "miniflare"
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.1.31",
		"@iconify-json/twemoji": "^1.1.15",
		"@picocss/pico": "^2.0.6",
		"@sveltejs/adapter-cloudflare": "^4.4.0",
		"@sveltejs/enhanced-img": "^0.3.9",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/markdown-it": "^14.1.1",
		"@types/markdown-it-attrs": "^4.1.3",
		"@types/proj4": "^2.5.5",
		"@unocss/extractor-svelte": "^0.59.4",
		"drizzle-kit": "^0.28.1",
		"marked": "^14.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-meta-tags": "^3.1.2",
		"tslib": "^2.4.1",
		"typescript": "^5.5.0",
		"ultramatter": "^0.0.4",
		"unocss": "^0.59.4",
		"vite": "^5.4.4",
		"vitest": "^2.1.8"
	},
	"type": "module",
	"dependencies": {
		"@libsql/client": "^0.14.0",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"drizzle-orm": "^0.36.2",
		"lucia": "^3.2.0",
		"marked-admonition-extension": "^0.0.4",
		"ol": "^9.1.0",
		"oslo": "^1.2.0",
		"proj4": "^2.11.0",
		"stripe": "^15.5.0",
		"sveltekit-superforms": "^2.14.0",
		"zod": "^3.23.8"
	},
	"imports": {
		"#drizzle-orm/libsql": {
			"development": "drizzle-orm/libsql",
			"default": "drizzle-orm/libsql/web"
		}
	}
}
